<!-- Configuration file used by Echoes of Eolis and FaxIScripts (https://github.com/kaimitai/faxedit) -->
<!-- Only make changes to this file if you are confident in what you are doing! (and keep backups even then) -->
<eoe_config app_version="beta-5">
	<regions>
		<!-- determine region by ROM signature - will take first match -->
		<!-- make sure to use identifiers that will not change via ROM editing -->
		<region name="en-transl" file_size="524304">
			<!-- look for "Dialogue" left behind after dialogue section relocation -->
			<signature no="0" offset="0x35f50" values="$44,$69,$61,$6c,$6f,$67,$75,$65" />
		</region>
		<region name="randum">
			<!-- look for "RANDUMIZER" -->
			<signature no="0" offset="0x31ddc" values="$f1,$e0,$ed,$e3,$f4,$ec,$e8,$f9,$e4,$f1" />
		</region>
		<region name="us">
			<!-- look for address of script ptr table -->
			<signature no="0" offset="0x3025d" values="$6b,$9f" />
		</region>
		<!-- look for address of script ptr table -->
		<!-- this might also apply to the revised US version but that is unproblematic -->
		<region name="eu">
			<signature no="0" offset="0x3025d" values="$b6,$9f" />
		</region>
		<!-- look for address of script ptr table -->
		<region name="jp">
			<signature no="0" offset="0x3025d" values="$67,$a3" />
		</region>
	</regions>

	<!-- all constants needed by the applications, looked up by region name -->
	<!-- if regions is not given, it will be an automatic match -->
	<!-- only the first match for any given ID will be used, so filter in the correct order -->

	<!-- offsets of pointer tables -->
	<pointers>
		<pointer name="metadata_ptr" value="0xc010" zero_addr="0xc010" />
		<pointer name="sprite_ptr" value="0x2c220" zero_addr="0x24010" />
		<pointer name="sameworld_trans_ptr" value="0x3ea47" zero_addr="0x30010" />
		<pointer name="otherworld_trans_ptr" value="0x3eaac" zero_addr="0x30010" />
		<!-- ROM offset of iScript pointer table lo bytes; combined with iscript_count we can dedude hi byte offset and script data start -->
		<pointer name="iscript_ptr_lo" region="jp,en-transl" value="0x32377" zero_addr="0x28010" />
		<pointer name="iscript_ptr_lo" region="us,randum" value="0x31f7b" zero_addr="0x28010" />
		<pointer name="iscript_ptr_lo" region="eu" value="0x31fc6" zero_addr="0x28010" />
		<!-- ROM offset to music data - ptr table -->
		<pointer name="music_ptr" region="us,randum,jp,en-transl" value="0x14f0b" zero_addr="0xc010" />
		<pointer name="music_ptr" region="eu" value="0x14f5a" zero_addr="0xc010" />
	</pointers>
	<!-- contant addresses and counts -->
	<!-- data end offsets will point to the first byte after the end, so data size is (end - start) -->
	<consts>
		<!-- mappings from world to tilemap bank, followed by mapping to ptr index -->
		<const name="world_tilemap_metadata" value="0x3df44" />
		<!-- max size screen tilemaps can occupy within a bank -->
		<const name="world_tilemap_max_size" value="0x4000" />
		<!-- end offsets for dynamic data -->
		<const name="metadata_end" value="0x10010" />
		<const name="sprite_data_end" value="0x30010" />
		<!-- randumizer uses free space for functions, we need to keep the original limits -->
		<!-- if these constants are defined for a region we keep original limits -->
		<!-- otherwise we use transition_data_start and transition_data_end -->
		<const name="sameworld_trans_data_end" region="randum" value="0x3ea60" />
		<const name="otherworld_trans_data_end" region="randum" value="0x3eb3f" />
		<!-- we combine same-world and other-world transition data if we use the bank 15 free space -->
		<const name="transition_data_start" region="en-transl" value="0x3fe0a" />
		<const name="transition_data_start" region="jp" value="0x3fdc2" />
		<const name="transition_data_start" region="eu,us" value="0x3fcdd" />
		<const name="transition_data_end" value="0x3fff0" />
